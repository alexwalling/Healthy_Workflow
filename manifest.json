{
	"manifest_version": 2,

	"name": "HealthyWorkflow",
	"description": "This extension keeps you on task and healthy during your workflow",
	"version": "2.0",
	"icons": { "16": "img/icon.png",
           		"48": "img/icon.png",
          		"128": "img/icon.png" },
	"options_page": "options.html",
	"browser_action": {
   		"default_icon": "img/icon.png",
   		"default_popup": "popup.html"
  	},
	"background": {
		"scripts":["background.js"]
	},
	"permissions": [
		"storage",
		"notifications",
		"activeTab",
		"https://ajax.googleapis.com/"
	]
}
